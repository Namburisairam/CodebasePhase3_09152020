@model EventManagement.DataAccess.DataBase.Model.Attende

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="content-wrapper">
    <section class="content">
        <div class="row spaceing">
            <div class="col-md-2"></div>
            <div class="col-md-8">
                <div class="box box-primary">
                    <div class="box-header with-border">
                        <div class="col-md-10">

                            <h3 class="box-title">Attendee</h3>
                        </div>
                        <div class="col-md-2">
                            @Html.ActionLink("Edit", "Create", new { id = Model.ID }) |
                            @Html.ActionLink("Back to List", "Index")
                        </div>
                    </div>

                    <div class="form-horizontal">

                        <div class="form-group">
                            @Html.LabelFor(model => model.FirstName, htmlAttributes: new { @class = "control-label col-md-2" })
                            <div class="col-md-10">
                                <h5>@Html.DisplayFor(model => model.FirstName)</h5>
                            </div>
                        </div>
                        <div class="form-group">
                            @Html.LabelFor(model => model.Lastname, htmlAttributes: new { @class = "control-label col-md-2" })
                            <div class="col-md-10">
                                <h5>@Html.DisplayFor(model => model.Lastname)</h5>
                            </div>
                        </div>
                        <div class="form-group">
                            @Html.LabelFor(model => model.CompanyName, htmlAttributes: new { @class = "control-label col-md-2" })
                            <div class="col-md-10">
                                <h5>@Html.DisplayFor(model => model.CompanyName)</h5>
                            </div>
                        </div>
                        <div class="form-group">
                            @Html.LabelFor(model => model.Description, htmlAttributes: new { @class = "control-label col-md-2" })
                            <div class="col-md-10">
                                <h5>@Html.Raw(Model.Description)</h5>
                                @*<h5>@Html.DisplayFor(model => model.Description)</h5>*@
                            </div>
                        </div>

                        <div class="form-group">
                            @Html.LabelFor(model => model.Thumbnail, htmlAttributes: new { @class = "control-label col-md-2" })
                            <div class="col-md-10">
                                <h5>@Html.DisplayFor(model => model.Thumbnail)</h5>
                            </div>
                        </div>

                        <div class="form-group">
                            @Html.LabelFor(model => model.FacebookURL, htmlAttributes: new { @class = "control-label col-md-2" })
                            <div class="col-md-10">
                                <h5>@Html.DisplayFor(model => model.FacebookURL)</h5>
                            </div>
                        </div>

                        <div class="form-group">
                            @Html.LabelFor(model => model.TwitterURL, htmlAttributes: new { @class = "control-label col-md-2" })
                            <div class="col-md-10">
                                <h5>@Html.DisplayFor(model => model.TwitterURL)</h5>
                            </div>
                        </div>

                        <div class="form-group">
                            @Html.LabelFor(model => model.InstagramURL, htmlAttributes: new { @class = "control-label col-md-2" })
                            <div class="col-md-10">
                                <h5>@Html.DisplayFor(model => model.InstagramURL)</h5>
                            </div>
                        </div>

                        <div class="form-group">
                            @Html.LabelFor(model => model.AddedON, htmlAttributes: new { @class = "control-label col-md-2" })
                            <div class="col-md-10">
                                <h5>@Html.DisplayFor(model => model.AddedON)</h5>
                            </div>
                        </div>

                        <div class="form-group">
                            @Html.LabelFor(model => model.AddedBY, htmlAttributes: new { @class = "control-label col-md-2" })
                            <div class="col-md-10">
                                <h5>@Html.DisplayFor(model => model.AddedBY)</h5>
                            </div>
                        </div>

                        <div class="form-group">
                            @Html.LabelFor(model => model.Status, htmlAttributes: new { @class = "control-label col-md-2" })
                            <div class="col-md-10">
                                <h5>@Html.DisplayFor(model => model.Status)</h5>
                            </div>
                        </div>

                        <div class="form-group">
                            @Html.LabelFor(model => model.DeviceToken, htmlAttributes: new { @class = "control-label col-md-2" })
                            <div class="col-md-10">
                                <h5>@Html.DisplayFor(model => model.DeviceToken)</h5>
                            </div>
                        </div>

                        <div class="form-group">

                        </div>

                    </div>
                </div>
            </div>

            <div class="col-md-2"></div>
        </div>
    </section>
</div>