        function TemplateQuestionfunction()
        {
           $(".EditTemplateQuestion").click(function () {
           $("#AddOptions").empty();
               var TQID = $(this).attr("data-id");
                $("#TSurveyID").val('');
               var temp_TID = $("#Templateid").val();
               QuestionData = @{@Html.Raw(Json.Encode(ViewBag.TemplateQuestionOptions))};
               console.log(QuestionData);
               for (var i = 0; i < QuestionData.length; i++)
               {
                   if (QuestionData[i].QID == TQID)
                   {
                       $("#TSurveyID").val(temp_TID);
                       $('#TQuestionText').val(QuestionData[i].QuestionText);
                       $('#ResponseType').val(QuestionData[i].QuestionResponseType);
                       for (var j = 0; j < QuestionData[i].options.length; j++)
                       {
                           QuestionwithOptions(QuestionData[i].options[j].OptionText, QuestionData[i].options[j].OptionID);
                       };
                       $('#TemplateQuestionID').val(QuestionData[i].QID);
                   }
               }
               DeleteTemplateoptionsfunction();
            });
        }

        function QuestionwithOptions(text,index)
           {
               //var optiondata = $('<input class="form-control text-box single-line" name="Options[' + index + ']"/>').val(text);
            var optiondata = $('<input class="form-control text-box single-line" name="Options"/>').val(text);
            $('#AddOptions').append(optiondata).append('<input type="button" class="btn btn-danger DeleteTemplateOption" value="Delete" data-id=' + index + ' />');
        };

        function SurveyQuestionfunction() {

            $(".EditSurveyQuestion").click(function () {
                $("#AddSurveyQuestionOptions").empty();
                $("#S_QID").val('');
                var Q_surveyID = $("#SurveyID").val()
                $("#AddSurveyQuestionsBtn").empty();
                 var SQID = $(this).attr("data-id");
                 SQuestionData = @{@Html.Raw(Json.Encode(ViewBag.SurveyQuestionOptions))};
                 for (var i = 0; i < SQuestionData.length; i++)
                 {
                     if (SQuestionData[i].QID == SQID)
                     {
                         $("#S_QID").val(Q_surveyID);
                         $("#SurveyQuestionID").val(SQuestionData[i].QID);
                         $('#SQuestionText').val(SQuestionData[i].QuestionText);
                         $('#SQuestionResponseType').val(SQuestionData[i].QuestionResponseType);
                         for (var j = 0; j < SQuestionData[i].options.length; j++)
                         {
                             SurveyQuestionwithOptions(SQuestionData[i].options[j].OptionText, SQuestionData[i].options[j].OptionID);
                         };

