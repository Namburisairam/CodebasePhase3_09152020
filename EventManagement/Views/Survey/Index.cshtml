                     }
                }
                DeleteSurveyoptionsfunction();
            });
        };

        function SurveyQuestionwithOptions(text, index) {
            //var optiondata = $('<input class="form-control text-box single-line" name="SOptions[' + index + ']"/>').val(text);

             var optiondata = $('<input class="form-control text-box single-line" name="SOptions"/>').val(text);

