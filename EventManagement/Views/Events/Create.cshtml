        $('#saveWidgetChanges').click(function () {
            var saveData=[];
            $('.grid-stack-item.ui-draggable').each(function () {
        var $this = $(this);
        saveData.push({
            XLocation: $this.attr('data-gs-x'),
            YLocation: $this.attr('data-gs-y'),
            Width: $this.attr('data-gs-width'),
            Height: $this.attr('data-gs-height'),
            WidgetID: $this.attr('data-widget-id'),
            ID: $this.attr('data-id'),
            EventID: @Model.ID
        });
            });
            $('#widgetsData').val(JSON.stringify(saveData));
        });

            $('.widget-edit').click(
                function () {
                    var widgetID = parseInt($(this).closest('.grid-stack-item').attr('data-id'));
                    var widgetObj = _.find(items, function (item) {
                        return item.ID === widgetID
                    });
                    var rootImagePath = '@MvcCustom.RootImagePath()';
                    $('#widgetId').val(widgetID);
                    $('#WidgetDisplayName').val(widgetObj.DisplayName);
                    $('#screenType').val(widgetObj.WidgetID).trigger("chosen:updated");
                    $('#widgetType').val(widgetObj.widgetTypeID).trigger("chosen:updated");

                    $('#widgetImagePreview').attr('src',  widgetObj.ImagePath ? rootImagePath + "/" + widgetObj.ImagePath :'/Content/NoImage.png' );
                    //$('#isWidgetExternalLink').val(widgetObj.isExternalLink);
                    $('#isWidgetExternalLink').prop('checked', widgetObj.isExternalLink);
                    $('#widgetExternalLink').val(widgetObj.ExternalLink);
                    $('#fontSize').val(widgetObj.FontSize);
                    $('#fontColor').val(widgetObj.Color || "").trigger("change");
                    $('#justification').val(widgetObj.Justification);
                    handleWidgetChange(widgetObj.widgetTypeID,widgetObj.ImagePath);
                });
            $('#addWidget').click(function () {
                $('#widgetId').val('');
                $('#WidgetDisplayName').val('');
                $('#widgetType').prop("selectedIndex", 0).trigger("chosen:updated");
                $('#screenType').val('').trigger("chosen:updated");
                $('#isWidgetExternalLink').prop('checked', false);
                $('#widgetImagePreview').attr('src', '/Content/NoImage.png');
                $('#widgetExternalLink').val('');
                handleWidgetChange($('#widgetType').val());
            });

            $('#widgetType').change(
                function () {
                    var widgetTypeID = $(this).val();
                    handleWidgetChange(widgetTypeID);
                });

            $('#isWidgetExternalLink').change(function () {
                $('#screenType').val(null).trigger("chosen:updated");
                handleExternalLinkVisibility();
            });

            function handleWidgetChange(widgetTypeID, ImagePath ) {
                if (widgetTypeID == 1) {
                    $('.image-type').show();
                    $('#widgetImage').prop("required", !ImagePath);
                    handleExternalLinkVisibility();
                } else if (widgetTypeID == 2) {
                    $('.image-type').hide();
                    $('#widgetImage').prop("required", false);
                    $('#screenTypeContainer').show();
                }
            }

            function handleExternalLinkVisibility() {
                if (! $('#isWidgetExternalLink').is(':checked')) {
                        $('#widgetExternalLink').hide();
                    $('#screenTypeContainer').show();
                    $('#screenType').prop("required", true);
                    } else {
                        $('#widgetExternalLink').show();
                    $('#screenTypeContainer').hide();
                    $('#screenType').prop("required", false);
                    }
            }

            $('#homePageConfigForm').submit(function (event) {
                var widgetScreenTypeIDs = $('.grid-stack-item.ui-draggable').map(function () {
                    var $this = $(this);
                    return { WSID : $this.attr('data-widget-id') , WID : $this.attr('data-id') };
                });
                var widgetID = $('#widgetId').val();
                var hasDuplicates = _.any(widgetScreenTypeIDs, function (data) {
                    return data.WSID === $('#screenType').val() && data.WID !==widgetID ;
              });
                if (hasDuplicates) {
                    alert("Cannot add duplicate widgets");
                    return false;
                }
            });



