                                <script>
                                    var options = {
                                        center: [@Model.latitude,@Model.longitude],
                                        zoom: 10
                                        };
                                    if (options.center == "")
                                    {
                                      options.center = [37.7880, -122.4075]
                                    }
                                    var map = L.map('map', options).setView(options.center, 15);

                                    L.tileLayer('http://{s}.tile.osm.org/{z}/{x}/{y}.png', { attribution: false })
                                        .addTo(map);
                                    var firefoxIcon = L.icon({
                                        //className: 'custom-div-icon',
                                        //html: "<div style='background-color:#4838cc;' class='marker-pin'></div><i class='fa fa-camera awesome'>",
                                        //iconSize: [30, 42],
                                        //iconAnchor: [15, 42]
                                        iconUrl: 'https://cdn.imgbin.com/23/6/2/imgbin-google-maps-pin-bing-maps-mapquest-map-XYjGHLZTTCX40ryeg1LqzvGbe.jpg',
                                        ////shadowUrl: 'data:image/png;base64,',
                                        iconSize: [25, 45],
                                        //shadowSize: [45, 60]
                                    });
                                        var coordinates = JSON.parse('@Html.Raw(Json.Encode(Model.EventLocationDetails.Select(x => new { x.Coordinates, x.LocationDescription,x.ID ,x.Name,x.Webpageurl,x.MarkerImage1,x.MarkerImage2, x.WebPageName})))');
                                        for (var i = 0; i <= coordinates.length - 1; i++) {
                                            var myMarker = L.marker(JSON.parse(coordinates[i].Coordinates),
                                                {
                                                icon: firefoxIcon,
                                                title: coordinates[i].LocationDescription,
                                                alt: "marker",
                                                MarkerId: coordinates[i].ID,
                                                markercoordinates: coordinates[i].Coordinates,
                                                markerdetails: coordinates[i].LocationDescription,
                                                name: coordinates[i].Name,
                                                webpageurl: coordinates[i].Webpageurl,
                                                markerimage1: coordinates[i].MarkerImage1,
                                                markerimage2: coordinates[i].MarkerImage2,
                                                webpagename : coordinates[i].WebPageName
                                            }).addTo(map);

                                                  myMarker.clicked = 0;
                                                  myMarker.on('click', onclick);
                                                  myMarker.on('dblclick', ondblclick);
                                                }

                                        function onclick() {
                                            myMarker.clicked = myMarker.clicked + 1;
                                            var self = this;
                                            setTimeout(function () {
                                                if (myMarker.clicked == 1) {
                                                    $("#ID").val(self.options.MarkerId);
                                                    $("#mapcoordinates").val(self.options.markercoordinates);
                                                    $("#LocationInfo").val(self.options.markerdetails);
                                                    $("#EventMarkerName").val(self.options.name);
                                                    $("#Webpage").val(self.options.webpageurl);
                                                    $("#WebpageName").val(self.options.webpagename)
                                                    var img1 = '@MvcCustom.RootImagePath()' + '/' + self.options.markerimage1;
                                                    $("#mimage1").attr('src', img1);
                                                    console.log(img1);
                                                    //$("#Markerimage1").val();
                                                    var img2 = '@MvcCustom.RootImagePath()' + '/' + self.options.markerimage2;
                                                    $("#mimage2").attr('src', img2);
                                                    //$("#Markerimage2").val(self.options.markerimage2);
                                                    $('#AddLocationInfo').modal('show');
                                                    myMarker.clicked = 0;
                                              }
                                            }, 500);
                                            }
                                    function ondblclick() {
                                         myMarker.clicked = 0;
                                        $("#mapcoordinates1").val(this.options.markercoordinates);
                                        $("#LocationInfo1").val(this.options.markerdetails);
                                         $('#RemoveMarker').modal('show');
                                         //map.removeLayer(this);

                                    }

                                    function onMapClick(e) {
                                        var coord = String(e.latlng).split(',');
                                        var locationcoords = '[' + [coord[0].split('(')[1], coord[1].split(')')[0]] + ']';
                                        $("#mapcoordinates").val(locationcoords);
                                            $('#AddLocationInfo').modal('show');
                                            $("#ID").val('');
                                            $("#LocationInfo").val('');
                                            $("#EventMarkerName").val('');
                                            $("#Webpage").val('');
                                            $("#WebpageName").val('');
                                            $("#mimage1").remove();
                                            $("#mimage2").remove();
                                     var geocodeService = L.esri.Geocoding.geocodeService();
                                     geocodeService.reverse().latlng([coord[0].split('(')[1], coord[1].split(')')[0]])
                                            .run(function (error, result) {
                                                L.marker(result.latlng,
                                                    { icon: firefoxIcon, title: result.address.Match_addr })
                                                    .addTo(map)
                                                    .bindPopup(result.address.Match_addr)
                                                    .openPopup()
                                            });
                                    }


                                    function addingLocationThroughSearchBar() {

                                        var mapLat = document.getElementById("Maplat").value;
                                        var mapLang = document.getElementById("Maplang").value;
                                        var locationName = document.getElementById("MapAddress").value;

                                        if (locationName == '') {
                                            alert("Please provide location name");
                                            return;
                                        }

                                        if (mapLat == '' || mapLang) {
                                            alert("Please provide valid location name");
                                            return;
                                        }

                                        var locationcoords = '[' + mapLat+', '+ mapLang + ']';
                                        $("#mapcoordinates").val(locationcoords);
                                            $('#AddLocationInfo').modal('show');
                                            $("#ID").val('');
                                            $("#LocationInfo").val('');
                                            $("#EventMarkerName").val(locationName);
                                            $("#Webpage").val('');
                                            $("#WebpageName").val('');
                                            $("#mimage1").remove();
                                            $("#mimage2").remove();
                                     var geocodeService = L.esri.Geocoding.geocodeService();
                                     geocodeService.reverse().latlng(mapLat, mapLang)
                                            .run(function (error, result) {
                                                L.marker(result.latlng,
                                                    { icon: firefoxIcon, title: result.address.Match_addr })
                                                    .addTo(map)
                                                    .bindPopup(result.address.Match_addr)
                                                    .openPopup()
                                            });
                                         }

                                    map.on('click', onMapClick);

                                </script>
